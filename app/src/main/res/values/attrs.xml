<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
        <attr name="gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RadarLayout">
        <attr name="CircleCount" format="integer"/>
        <attr name="CircleColor" format="color"/>
        <attr name="Duration" format="integer"/>
        <attr name="StartRatio" format="float"/>
    </declare-styleable>
</resources>